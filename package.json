{
  "name": "ptitpote",
  "private": true,
  "version": "1.0.0",
  "description": "a discord bot",
  "main": "api.js",
  "type": "module",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "start:api": "node dist/src/api.js",
    "start:gateway": "node dist/src/gateway.js",
    "register": "node dist/src/register.js",
    "dev:api": "nodemon -q dist/src/api.js",
    "dev:gateway": "nodemon -q dist/src/gateway.js",
    "test": "vitest --config ./vitest.config.ts run",
    "build": "tsc --pretty",
    "lint": "prettier . --check"
  },
  "mikro-orm": {
    "useTsNode": false,
    "configPaths": [
      "./dist/src/mikro-orm.config.js"
    ]
  },
  "author": "GTSpray",
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/cli": "^6.6.3",
    "@mikro-orm/core": "^6.5.4",
    "@mikro-orm/mariadb": "^6.6.3",
    "@mikro-orm/migrations": "^6.6.3",
    "@mikro-orm/reflection": "^6.6.3",
    "bufferutil": "^4.1.0",
    "discord-interactions": "^4.4.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "morgan": "^1.10.1",
    "utf-8-validate": "^6.0.6",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "discord-api-types": "^0.38.37",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.11",
    "prettier": "3.7.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
